{
	"version": 1,
	"cli": {
		"analytics": false,
		"defaultCollection": "@nrwl/angular"
	},
	"defaultProject": "demo-player-angular",
	"schematics": {
		"@nrwl/angular": {
			"application": {
				"linter": "eslint"
			},
			"library": {
				"linter": "eslint"
			},
			"storybook-configuration": {
				"linter": "eslint"
			}
		},
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {
			"style": "scss"
		},
		"@nrwl/react": {
			"application": {
				"style": "scss",
				"linter": "eslint",
				"babel": true
			},
			"component": {
				"style": "scss"
			},
			"library": {
				"style": "scss",
				"linter": "eslint"
			}
		}
	},
	"projects": {
		"demo-player-angular": {
			"projectType": "application",
			"root": "apps/demo-player-angular",
			"sourceRoot": "apps/demo-player-angular/src",
			"prefix": "rumblestudio",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/apps/demo-player-angular",
						"index": "apps/demo-player-angular/src/index.html",
						"main": "apps/demo-player-angular/src/main.ts",
						"polyfills": "apps/demo-player-angular/src/polyfills.ts",
						"tsConfig": "apps/demo-player-angular/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"apps/demo-player-angular/src/favicon.ico",
							"apps/demo-player-angular/src/assets"
						],
						"styles": ["apps/demo-player-angular/src/styles.scss"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/demo-player-angular/src/environments/environment.ts",
									"with": "apps/demo-player-angular/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "demo-player-angular:build:production"
						},
						"development": {
							"browserTarget": "demo-player-angular:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "demo-player-angular:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/demo-player-angular/src/**/*.ts",
							"apps/demo-player-angular/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/demo-player-angular"],
					"options": {
						"jestConfig": "apps/demo-player-angular/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"demo-player-angular-e2e": {
			"root": "apps/demo-player-angular-e2e",
			"sourceRoot": "apps/demo-player-angular-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/demo-player-angular-e2e/cypress.json",
						"tsConfig": "apps/demo-player-angular-e2e/tsconfig.e2e.json",
						"devServerTarget": "demo-player-angular:serve:development"
					},
					"configurations": {
						"production": {
							"devServerTarget": "demo-player-angular:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/demo-player-angular-e2e/**/*.{js,ts}"
						]
					}
				}
			}
		},
		"player-service": {
			"root": "libs/player-service",
			"sourceRoot": "libs/player-service/src",
			"projectType": "library",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/player-service/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/player-service"],
					"options": {
						"jestConfig": "libs/player-service/jest.config.js",
						"passWithNoTests": true
					}
				},
				"build": {
					"builder": "@nrwl/node:package",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/libs/player-service",
						"tsConfig": "libs/player-service/tsconfig.lib.json",
						"packageJson": "libs/player-service/package.json",
						"main": "libs/player-service/src/index.ts",
						"assets": ["libs/player-service/*.md"]
					}
				}
			}
		},
		"demo-player-angular-spotify": {
			"projectType": "application",
			"root": "apps/demo-player-angular-spotify",
			"sourceRoot": "apps/demo-player-angular-spotify/src",
			"prefix": "rumblestudio",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/apps/demo-player-angular-spotify",
						"index": "apps/demo-player-angular-spotify/src/index.html",
						"main": "apps/demo-player-angular-spotify/src/main.ts",
						"polyfills": "apps/demo-player-angular-spotify/src/polyfills.ts",
						"tsConfig": "apps/demo-player-angular-spotify/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"apps/demo-player-angular-spotify/src/favicon.ico",
							"apps/demo-player-angular-spotify/src/assets"
						],
						"styles": [
							"./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
							"apps/demo-player-angular-spotify/src/styles.scss"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/demo-player-angular-spotify/src/environments/environment.ts",
									"with": "apps/demo-player-angular-spotify/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "demo-player-angular-spotify:build:production"
						},
						"development": {
							"browserTarget": "demo-player-angular-spotify:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "demo-player-angular-spotify:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/demo-player-angular-spotify/src/**/*.ts",
							"apps/demo-player-angular-spotify/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/demo-player-angular-spotify"],
					"options": {
						"jestConfig": "apps/demo-player-angular-spotify/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"demo-player-angular-spotify-e2e": {
			"root": "apps/demo-player-angular-spotify-e2e",
			"sourceRoot": "apps/demo-player-angular-spotify-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/demo-player-angular-spotify-e2e/cypress.json",
						"tsConfig": "apps/demo-player-angular-spotify-e2e/tsconfig.e2e.json",
						"devServerTarget": "demo-player-angular-spotify:serve:development"
					},
					"configurations": {
						"production": {
							"devServerTarget": "demo-player-angular-spotify:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/demo-player-angular-spotify-e2e/**/*.{js,ts}"
						]
					}
				}
			}
		},
		"demo-player-react-spotify": {
			"root": "apps/demo-player-react-spotify",
			"sourceRoot": "apps/demo-player-react-spotify/src",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@nrwl/web:build",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/demo-player-react-spotify",
						"index": "apps/demo-player-react-spotify/src/index.html",
						"main": "apps/demo-player-react-spotify/src/main.tsx",
						"polyfills": "apps/demo-player-react-spotify/src/polyfills.ts",
						"tsConfig": "apps/demo-player-react-spotify/tsconfig.app.json",
						"assets": [
							"apps/demo-player-react-spotify/src/favicon.ico",
							"apps/demo-player-react-spotify/src/assets"
						],
						"styles": [
							"apps/demo-player-react-spotify/src/styles.scss"
						],
						"scripts": [],
						"webpackConfig": "@nrwl/react/plugins/webpack"
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "apps/demo-player-react-spotify/src/environments/environment.ts",
									"with": "apps/demo-player-react-spotify/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@nrwl/web:dev-server",
					"options": {
						"buildTarget": "demo-player-react-spotify:build"
					},
					"configurations": {
						"production": {
							"buildTarget": "demo-player-react-spotify:build:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/demo-player-react-spotify/**/*.{ts,tsx,js,jsx}"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/demo-player-react-spotify"],
					"options": {
						"jestConfig": "apps/demo-player-react-spotify/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"demo-player-react-spotify-e2e": {
			"root": "apps/demo-player-react-spotify-e2e",
			"sourceRoot": "apps/demo-player-react-spotify-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/demo-player-react-spotify-e2e/cypress.json",
						"tsConfig": "apps/demo-player-react-spotify-e2e/tsconfig.e2e.json",
						"devServerTarget": "demo-player-react-spotify:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "demo-player-react-spotify:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/demo-player-react-spotify-e2e/**/*.{js,ts}"
						]
					}
				}
			}
		}
	}
}
